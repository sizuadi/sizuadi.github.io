# Example environment variables for build-time (Create React App)
# Copy to .env and adjust values, then build with docker build --build-arg NAME=VALUE
# or via docker-compose build args. Only REACT_APP_* are embedded in bundle.

PUBLIC_URL=/
REACT_APP_API_BASE=https://api.example.com
REACT_APP_ENV=production
REACT_APP_ANALYTICS_ID=UA-XXXXX-Y
REACT_APP_FEATURE_FLAG=enabled
FAST_REFRESH=false
SKIP_PREFLIGHT_CHECK=true
WATCHPACK_POLLING=true
REACT_APP_BLOG_URL="https://blog.sizu.dev"